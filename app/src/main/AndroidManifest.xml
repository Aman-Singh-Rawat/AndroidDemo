<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/newslogo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewsApp"
        tools:targetApi="31">
        <activity
            android:name=".presenter.screen.onboading.TrendingDemo"
            android:exported="false" />
        <activity
            android:name=".presenter.screen.onboading.ProfileDemo"
            android:exported="false" />
        <activity
            android:name=".presenter.screen.onboading.ElevateDemo"
            android:exported="false" />
        <activity
            android:name=".presenter.screen.onboading.WelcomeActivity"
            android:exported="false" />
        <activity
            android:name=".presenter.screen.auth.SignUp"
            android:exported="false" />
        <activity
            android:name=".presenter.screen.splash.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>