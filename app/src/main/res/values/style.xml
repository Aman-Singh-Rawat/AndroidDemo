<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="myTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="textSize" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textSize">50sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/roboto</item>
    </style>
    <style name="headText" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="smallText" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/roboto_light</item>
    </style>
    <style name="singleBoldText" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="greenColorText" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/teal_700</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>


</resources>